{
  "name": "nomatic-events",
  "version": "1.0.4",
  "author": "Brian D. Foster <bdfoster89@gmail.com> (https://github.com/bdfoster)",
  "description": "Fast, asynchronous, and regex-enabled event framework for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bdfoster/nomatic-events"
  },
  "bugs": {
    "url": "https://github.com/bdfoster/nomatic-events/issues"
  },
  "runkitExampleFilename": "examples/runkit.js",
  "scripts": {
    "build": "tsc --diagnostics --listEmittedFiles",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "codeclimate": "npm test && nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "coverage",
      ".nyc_output",
      ".git",
      "test",
      "dist"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "mocha": "^3.0.2",
    "nyc": "^10.0.0",
    "ts-node": "^1.7.0",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.39",
    "@types/node": "^7.0.11",
    "lodash": "^4.15.0"
  },
  "keywords": [
    "EventEmitter",
    "EventListener",
    "event",
    "events",
    "emitter",
    "listener",
    "nomatic-lib",
    "nomatic",
    "observer",
    "pattern",
    "typescript"
  ]
}
